<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--此程序的服务名称：USBThiefService-->
    <add key="ServiceName" value="USBThiefService"/>
    <!--多少秒监听一次服务状态-->
    <add key="IntervalTime" value="5"/>
    <!--多少秒监听一次读取文件-->
    <add key="ReadTime" value="2"/>
    <!--目标存储文件路径-->
    <add key="copyPath" value="E:\USD"/>
  </appSettings>
</configuration>